import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter how many primes numbers to be printed");
        int n = sc.nextInt();
        for(int i=0; i<=n; i++)
        {
            System.out.println(i);
        }
    }
}
